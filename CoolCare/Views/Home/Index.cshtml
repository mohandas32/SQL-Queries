<!-- ======= Hero Section ======= -->
<section id="hero" class="clearfix">

    <div class="container" data-aos="fade-up">

        <div class="row">

            <div class="col-md-8 mt-5">
                <div class="hero-info" data-aos="zoom-in" data-aos-delay="100">
                    <h2>
                        We Provide<br><span>AC Repair & Service</span>
                        <br />
                    </h2>
                    <div class="mt-4">
                        @if (Session["userid"] == null)
                        {
                            <a href="/Login/UserLogin" class="btn-get-started scrollto">Book Now</a>

                        }
                        else
                        {
                            <a href="/Home/BookService" class="btn-get-started scrollto">Book Now</a>

                        }
                        <a href="#services" class="btn-services scrollto">Our Services</a>
                    </div>
                </div>
            </div>
            <div class="col-md-4 d-flex justify-content-center">
                <!-- <img src="assets/img/bg.jpg"  alt=""> -->
                <img src="~/Content/CoolCare/assets/img/Logo.PNG" width="165%" alt="">

            </div>
        </div>



    </div>

</section>
<!-- End Hero Section -->

<main id="main">

    <!-- ======= Services Section ======= -->
    <section id="services" class="section-bg">
        <div class="container" data-aos="fade-up">

            <header class="section-header">
                <h3>Services</h3>

                <p>
                    We are a team of specialists. We are your best and nearest option for fixing. Execution is our
                    favorite, and we assist you right at your doorstep with trust as our top priority.
                </p>
            </header>

            <div class="row justify-content-center">
                @*@foreach (var i in Model)
                    {*@

                @foreach (var item in Model.Services)
                {
                    <div class="col-md-6 col-lg-4" data-aos="zoom-in" data-aos-delay="100">
                        <div class="box" style="padding: 18px;">
                            @if (Session["userid"] == null)
                            {

                                <a href="/Login/UserLogin"><img src="~/Content/CoolCare/assets/img/acrepair.jpg" class="img-box" style="width: 100%;"></a>
                                <a href="/Login/UserLogin">
                                    <h4 class="title"
                                        style="display: flex; justify-content: center;  margin: 15px; font-size: 28px;">

                                        @item.ServiceName

                                    </h4>

                                </a>
                            }
                            else
                            {
                                <a href="/Home/BookService">
                                    <img src="~/Content/CoolCare/assets/img/acrepair.jpg" class="img-box" style="width: 100%;">
                                </a>

                                <a href="/Home/BookService">
                                    <h4 class="title"
                                        style="display: flex; justify-content: center;  margin: 15px; font-size: 28px;">

                                        @item.ServiceName

                                    </h4>
                                </a>
                            }
                            <h5 class="Price" style="margin: 0;">
                                @item.Price Rs.
                            </h5>
                            <h6 class="description" style="margin: 0;">
                                @item.ServiceDescription
                            </h6>
                        </div>
                    </div>
                }


            </div>

        </div>
    </section>
    <!-- End Services Section -->


    <!-- ======= Package Section ======= -->
