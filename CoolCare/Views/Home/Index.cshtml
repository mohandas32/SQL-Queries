    <!-- ======= Why Us Section ======= -->
    <section id="why-us">
        <div class="container" data-aos="fade-up">
            <header class="section-header">
                <h3>Why choose us?</h3>
                <p>
                    We are the most trusted choice when it comes to quick, speedy, simple, machine repair and support
                    at home.
                </p>
            </header>

            <div class="row row-eq-height justify-content-center">

                <div class="col-lg-4 mb-4">
                    <div class="card" data-aos="zoom-in" data-aos-delay="100">
                        <img src="~/Content/CoolCare/assets/img/acicon1.png" style="width: 28%;margin: auto;">

                        <div class="card-body">
                            <h5 class="card-title">Budget-friendly AC Repair Service from Experts</h5>
                            <p class="card-text">
                                We offer affordable air conditioning repair services
                                handled by experienced experts. We use the latest tools and techniques to ensure
                                timely and efficient service, and pride ourselves on providing quality repairs at
                                competitive rates.
                            </p>
                            <a href="javascript:void(0);" class="readmore">Read more </a>
                        </div>
                    </div>
                </div>

                <div class="col-lg-4 mb-4">
                    <div class="card" data-aos="zoom-in" data-aos-delay="200">
                        <img src="~/Content/CoolCare/assets/img/acicon2.png" style="width: 28%;margin: auto;">
                        <div class="card-body">
                            <h5 class="card-title">We Listen to Your Repair Requests</h5>
                            <p class="card-text">
                                For any AC issues, remember to contact us for fast and reliable
                                service. We're here to keep you cool and comfortable.
                            </p>
                            <a href="javascript:void(0);" class="readmore">Read more </a>
                        </div>
                    </div>
                </div>

                <div class="col-lg-4 mb-4">
                    <div class="card" data-aos="zoom-in" data-aos-delay="300">
                        <img src="~/Content/CoolCare/assets/img/acicon3.png" style="width: 22%;margin: auto;">
                        <div class="card-body">
                            <h5 class="card-title">AC Support 24x7 to assist you in the hour of need</h5>
                            <p class="card-text">
                                We provide exceptional customer service, which is why we offer 24/7
                                support to all customers who have signed up for our annual maintenance contract. Our
                                team is always available to listen to any concerns or queries you may have, and we
                                prioritize quick and effective solutions to keep your air conditioning system
                                running smoothly. Trust us for round-the-clock support and peace of mind
                            </p>
                            <a href="javascript:void(0);" class="readmore">Read more </a>
                        </div>
                    </div>
                </div>

            </div>

            <div class="row counters" data-aos="fade-up" data-aos-delay="100">

                <div class="col-lg-3 col-6 text-center">
                    <span data-purecounter-start="0" data-purecounter-end="1" data-purecounter-duration="1"
                          class="purecounter"></span>
                    <p>Years of superior service</p>
                </div>
                <div class="col-lg-3 col-6 text-center">
                    <span data-purecounter-start="0" data-purecounter-end="@ViewBag.usercount" data-purecounter-duration="1"
                          class="purecounter"></span>
                    <p>Total Customers</p>
                </div>


                <div class="col-lg-3 col-6 text-center">
                    <span data-purecounter-start="0" data-purecounter-end="@ViewBag.usercount" data-purecounter-duration="1"
                          class="purecounter"></span>
                    <p>Happy Customers</p>
                </div>

                <div class="col-lg-3 col-6 text-center">
                    <span data-purecounter-start="0" data-purecounter-end="@ViewBag.techniciancount" data-purecounter-duration="1"
                          class="purecounter"></span>
                    <p>Skilful Technicians</p>
                </div>

            </div>

        </div>
    </section>


    <!-- End Testimonials Section -->
    <!-- ======= Contact Section ======= -->
    <section id="contact">
        <div class="container-fluid" data-aos="fade-up">

            <div class="section-header2 mb-5">
                <h3>Contact Us</h3>
            </div>

            <div class="row">

                <div class="col-lg-6" style="display: none;">
                    <div class="map mb-4 mb-lg-0">
                        <iframe src="https://www.google.com/maps/embed?pb=!1m14!1m8!1m3!1d12097.433213460943!2d-74.0062269!3d40.7101282!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x0%3A0xb89d1fe6bc499443!2sDowntown+Conference+Center!5e0!3m2!1smk!2sbg!4v1539943755621"
                                frameborder="0" style="border:0; width: 100%; height: 340px;" allowfullscreen></iframe>
                    </div>
                </div>

                <div class="col-lg-8" style="margin: auto;">
                    <div class="row">
                        <div class="locationIcon col-md-5 info">
                            <i class="loc bi bi-geo-alt"></i>
                            <p class="iconText text-white ms-2">
                                Coolcare office, Palladium mall , S G highway
                                ahmedabad
                            </p>
                        </div>
                        <div class="mailIcon col-md-4 info">
                            <i class="mail bi bi-envelope"></i>
                            <p class="iconText text-white ms-2">coolcare@gmail.com</p>
                        </div>
                        <div class="phoneIcon col-md-3 info">
                            <i class="phone bi bi-phone"></i>
                            <p class="iconText text-white ms-2">+91-9157224422</p>
                        </div>
                    </div>





                    <div class="form">
                        @using (Html.BeginForm("ContactUs", "Home", FormMethod.Post, new { id = "formValidation", @class = "php-email-form", enctype = "multipart/form-data" }))
                        {
                            //foreach (var c in Model)
                            //{
                            <div class="row">
                                <div class="form-group col-lg-6">
                                    @Html.TextBoxFor(m => m.ContactU.Name, new { @class = "form-control", @placeholder = "Your Name" })
                                    @Html.ValidationMessageFor(m => m.ContactU.Name, "", new { @class = "text-danger" })
                                </div>
                                <div class="form-group col-lg-6 mt-3 mt-lg-0">
                                    @Html.TextBoxFor(m => m.ContactU.Email, new { @class = "form-control", @placeholder = "Your Email" })
                                    @Html.ValidationMessageFor(m => m.ContactU.Email, "", new { @class = "text-danger" })

                                </div>

                            </div>

                            <div class="form-group mt-3">
                                @Html.TextBoxFor(m => m.ContactU.PhoneNo, new { @class = "form-control", @placeholder = "Phone Number" })
                                @Html.ValidationMessageFor(m => m.ContactU.PhoneNo, "", new { @class = "text-danger" })

                            </div>

                            <div class="form-group mt-3">
                                @Html.TextAreaFor(m => m.ContactU.Message, new { @class = "form-control", @rows = "5", @placeholder = "Message" })
                                @Html.ValidationMessageFor(m => m.ContactU.Message, "", new { @class = "text-danger" })

                            </div>

                            <div class="my-3">

                                <div class="loading"> Loading </div>

                                <div class="error-message"></div>

                                <div class="sent-message">
                                    Your message has been sent. Thank you!
                                </div>
                            </div>
                            <div class="text-center mt-5">
                                <button type="submit" title="Send Message" class="sendMessage" name="submit_subadmin"> Send Message </button>

                            </div>
                        }


                    </div>

                </div>

            </div>
    </section>
    <!-- End Contact Section -->

</main>
